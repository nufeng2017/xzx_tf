/*轮播图容器*/
#imgBox{
  position: relative;
  margin-bottom:44rpx;
}
#imgBox .seehouse360{
  position:absolute;
  top:50%;
  left:50%;
  width:270rpx;
  height:94rpx;
  margin-left: -135rpx;
  margin-top: -47rpx;
  z-index: 5;
}
.overImg{
  position: absolute;
  top:0px;
  left:0px;
  height:100%;
  width:100%;
  z-index: 102;
}
#imgBox.shadow:after{
  content: '';
  display: block;
  position: absolute;
  bottom:0px;
  left:0px;
  width:100%;
  height:100rpx;
  background: -moz-linear-gradient(top, transparent,rgba(0, 0, 0, 0.2), rgba(0, 0, 0, 0.4));
  background: linear-gradient(top, transparent, rgba(0, 0, 0, 0.2),rgba(0, 0, 0, 0.4));
  background: -webkit-linear-gradient(top, transparent,rgba(0, 0, 0, 0.2), rgba(0, 0, 0, 0.4));
  background: -o-linear-gradient(top, transparent, rgba(0, 0, 0, 0.2),rgba(0, 0, 0, 0.4));
  z-index: 100;
}
#imgsNum,#uploadTime{
  position: absolute;
  bottom: 20rpx;
  right:30rpx;
  color:#fff;
  font-size:20rpx;
  z-index: 101;
}
#uploadTime{
