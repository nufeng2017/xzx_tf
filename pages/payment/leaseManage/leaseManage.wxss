page{
    background: #f4f4f4;
    font-size: 30rpx;
}
.title{
    height:88rpx;
    background: #fff;
    line-height: 88rpx;
    overflow: hidden;
    margin-bottom: 20rpx;
    display: flex;
    align-items: center;
    justify-content:space-between;
}
.title > view{
    flex-direction:column;
    text-align: center;
    width: 50%;
}
.title > view text{
    line-height: 82rpx;
    font-size: 30rpx;
    color: #333;
    display: block;
    width: 180rpx;
    margin: auto;
}
.title > view.active text{
    border-bottom: 6rpx solid #ffa229;
    color: #ffa229;
}
.list{
    /* width: 94.5%; */
    position: relative;
    width: 94.8%;
    margin: 0 2.6%;
    height: auto;
    overflow: hidden;
}
.list view.items{
    background: #fff;
    margin-bottom: 20rpx;
}
.list view.items .house_part{
    border-bottom: 1rpx solid #e5e5e5;
    padding: 30rpx 0 30rpx 30rpx;
    height: auto;
    overflow: hidden;
}
.list view.items .house_part > view{
    float: left;
}
.list view.items .house_part > view:first-child{
    margin-right: 34rpx;
    width: 210rpx;
    height: 160rpx;
}
.list view.items .house_part > view:first-child image{
    width: 100%;
    height: 100%;
}
.list view.items .house_part > view:last-child{
    width: 400rpx;
}
.list view.items .house_part > view:last-child text{
    display: block;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
    height: 53rpx;
    line-height: 53rpx;
}
.list view.items .house_part > view:last-child > text:first-child{
    font-size: 30rpx;
    color: #333;
    font-weight: bolder;
}
.list view.items .house_part > view:last-child > text:nth-child(2){
    font-size: 22rpx;
    color: #656565;
}
.list view.items .house_part > view:last-child > text:nth-child(3){
    font-size: 22rpx;
    color: #999;
}
.list view.items .house_part > view:last-child > text.rental{
  font-size: 30rpx;
  color: #ffa229;
}
.list view.items .house_part > view:last-child > text.rental text{
  font-size: 22rpx;
  display: inline;
}
.list view.items .house_part > view:last-child text text{
    margin-right: 10rpx;
    display: inline-block;
}
.list view.items .handle_part{
    padding: 30rpx 0 30rpx 40rpx;
    height: auto;
    overflow: hidden;
}
.list view.items .handle_part > view{
    float: left;
}
.list view.items .handle_part > view text:first-child{
    line-height: 30rpx;
    width: 110rpx;
    height: 30rpx;
    text-align: center;
    font-size: 20rpx;
    display: inline-block;
    border-radius: 4rpx;
}
.list view.items .handle_part > view text.paper{
    color: #31acd5;
    background: #e7faff;
}
.list view.items .handle_part > view text.dianzi{
    color: #7e97f2;
    background: #f2f0ff;
}
.list view.items .handle_part > view text:last-child{
    font-size: 24rpx;
    color: #ff0600;
    font-weight: normal;
    padding-left: 20rpx;
}
.list view.items .handle_part button {
    font-size: 24rpx;
    color: #ffa229;
    background: #fff;
    border: 0;
    height: 56rpx;
    line-height: 56rpx;
    text-align: center;
    margin-right: 18rpx;
    float: right;
}
.list view.items .handle_part button::after{
  border: 1px solid #ffa229;
}
.list view.items .handle_part button.checkpay {
    color: #444;
    background: #f8f8f8;
}
.list view.items .handle_part button.checkpay::after {
    border: 1px solid #dfdfdf;
}







