page{
    background: #f4f4f4;
    font-size: 30rpx;
    padding-bottom: 98rpx;
}
.list_con{
    background: #fff;
    padding-left: 40rpx;
    margin-bottom: 30rpx;
}
.list_con .list_item{
    padding: 30rpx 30rpx 40rpx 0;
    overflow: hidden;
    height: auto;
}
.list_con .list_item  view{
    float: left;
}
.list_con .list_item  view.item_left{
    width: 48rpx;
    height: 48rpx;
    box-sizing: content-box;
    padding-right: 24rpx;
    margin-top: 75rpx;
}
.list_con .list_item  view.item_left image{
    width: 48rpx;
    height: 48rpx;
}
.list_con .list_item  view.item_right{
    max-width: 597rpx;
}
.list_con .list_item  view.item_right > text:first-child{
    color: #333;
    font-size:30rpx;
    line-height: 40rpx;
    display: block;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
    font-weight: bold;
}
.list_con .list_item  view.item_right > text:nth-child(3){
    font-size:22rpx;
    line-height: 40rpx;
    color: #656565;
    padding: 10rpx 0 10rpx 0;
    display: block;
}
.list_con .list_item  view.item_right .housedetail{
    font-size:22rpx;
    line-height: 40rpx;
    color: #656565;
    padding: 10px 0 0 0;
    display: block;
}
.list_con .list_item  view.item_right > text text{
    padding-right: 10rpx;
}
.list_con .list_item  view.item_right .paper view{
    line-height: 30rpx;
    text-align: center;
    font-size: 24rpx;
    display: inline-block;
    border-radius: 4rpx;
    color: #31acd5;
    background: #e7faff;
    margin-right: 16rpx;
    padding: 0 10rpx;
}
.list_con .list_item  view.item_right .dianzi view{
    line-height: 30rpx;
    text-align: center;
    font-size: 24rpx;
    display: inline-block;
    border-radius: 4rpx;
    color: #7e97f2;
    background: #f2f0ff;
    margin-right: 16rpx;
    padding: 0 10rpx;
}
.list_con .list_item  view.item_right .dianzi view.red{
    color: #ff0600;
    background: transparent;
}
.pay_state_title {
    padding: 0 24rpx;
    background: #fff;
    height: 88rpx;
    line-height: 88rpx;
    overflow: hidden;
}
.pay_state_title > view{
    float: left;
    width: 33%;
    text-align: center;
    font-size: 30rpx;
    color: #333;
    box-sizing: border-box;
    height: 88rpx;
}
.pay_state_title > view.active {
    border-bottom: none;
    color: #ffa229;
}
.pay_state_title > view text{
    padding: 0 26rpx;
    height: 82rpx;
    display: inline-block;
}
.pay_state_title > view.active text {
    border-bottom: 6rpx solid #ffa229;
}
.pay_state {
    background: #fff;
    width: 100%;
    height: auto;
}
.pay_state .allpaids .paid_items{
    display: none;
}
.pay_state .allpaids .paid_items:first-child{
    display: block;
}
.pay_state .allpaids.showallpaids .paid_items{
    display: block;
}
.pay_state .paid_items .paid_items_time{
    color: #4c4c4c;
    padding: 32rpx 0 0 40rpx;
    font-size: 24rpx;
    line-height: 40rpx;
}
.pay_state .paid_items .paid_items_con{
    padding-left: 40rpx;
    height: auto;
    overflow: hidden;
}
.pay_state .paid_items .paid_items_con > view{
    padding:  40rpx 0;
    float: left;
}
.pay_state .paid_items .paid_items_con > view:first-child{
    width:40rpx;
    height: 40rpx;
    margin-right: 30rpx;
    margin-top: 25rpx;
    position: relative;
}
.pay_state .paid_items .paid_items_con > view:last-child{
    width: 608rpx;
    height: auto;
    overflow: hidden;
}
.pay_state .paid .paid_items .paid_items_con > view:last-child{
    width: 680rpx;
}
.pay_state .paid_items .paid_items_con > view .circle{
    width: 40rpx;
    height: 40rpx;
    border-radius: 50%;
    display: inline-block;
    border: 1rpx solid #c9c9c9;
    box-sizing: border-box;
}
.pay_state .paid_items .paid_items_con > view[data-selected='true'] .circle+image{
    width: 40rpx;
    height: 40rpx;
    position: absolute;
    right: 0;
}
.pay_state .paid_items .paid_items_con > view[data-selected='false'] .circle+image{
    display: none;
}
.pay_state .paid_items .paid_items_con > view view text{
    display: block;
    line-height: 40rpx;
}
.pay_state .paid_items .paid_items_con > view view.fl text:first-child{
    font-weight: bold;
}
.pay_state .paid_items .paid_items_con > view view.fr{
    margin-top: 12rpx;
}
.pay_state .paid_items .paid_items_con > view view.fr text{
    text-align: right;
}
.unpaid .seeAllpaid {
    background: #f4f4f4;
    width: 100%;
    font-size: 24rpx;
    color: #4b96ff;
    text-align: center;
    line-height: 80rpx;
    margin-bottom: 200rpx;
}
.unpaid .seeAllpaid image{
    width: 20rpx;
    height: 12rpx;
    padding-left:12rpx;
}
.paidbtn{
    position: fixed;
    height: 98rpx;
    background: #fff;
    padding-left: 40rpx;
    bottom: 0;
    left: 0;
    right: 0;
}
.paidbtn > view:nth-child(1){
    line-height: 98rpx;
    position: relative;
}
.paidbtn > view:nth-child(2){
    margin-left: 24rpx;
    padding-top: 20rpx;
}
.paidbtn > view:nth-child(2) text{
    display: block;
    line-height: 40rpx;
}
.paidbtn > view:last-child{
    height: 100%;
    width: 34.6%;
    text-align: center;
    line-height: 98rpx;
    color: #fff;
    font-size: 32rpx;
    background: #ffa229;
}
.paidbtn .circle{
    width: 40rpx;
    height: 40rpx;
    border-radius: 50%;
    border: 1rpx solid #c9c9c9;
    box-sizing: border-box;
    margin-right: 10rpx;
    margin-top: 32rpx;
    float: left;
    display: block;
}
.paidbtn .circle[data-selected='true']+image{
    width: 40rpx;
    height: 40rpx;
    position: absolute;
    left: 0;
    top: 32rpx;
}
.paidbtn .circle[data-selected='false']+image{
    display: none;
}












