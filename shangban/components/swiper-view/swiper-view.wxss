.z-component-swiper-view{
  width:100%;
  height: 100%;
  position: relative;
  overflow: hidden;
}
.z-component-swiper-img-item:after,.z-component-swiper-img-item:before{
  content: '';
  position: absolute;
  width:100%;
  height: 1rpx;
  bottom: 0;
  left: 0;
  box-shadow: 0 -60rpx 100rpx 60rpx rgba(0,0,0,0.4);
}
.z-component-swiper-img-item:before{
  top:0;
  box-shadow: 0 60rpx 100rpx 60rpx rgba(0,0,0,0.4);
}
.z-component-swiper,.z-component-swiper-item,.z-component-swiper-item-video{
  width:100%;
  height: 100%;
  position: relative;
}
.z-component-swiper-contrl-box{
  position: absolute;
  width:100%;
  height: 44rpx;
  line-height: 44rpx;
  color:#000;
  font-size: 26rpx;
  text-align: center;
  bottom: 15rpx;
  left: 0;
}
.z-component-swiper-contrl{
  background:rgba(255,255,255,0.7);
  border-radius: 50rpx;
  overflow: hidden;
  display: inline-block;
  height: 100%;
}
.z-component-swiper-contrl .z-component-swiper-contrl-item{
  float:left;
  padding:0 20rpx;
}
.z-component-swiper-contrl .z-component-swiper-contrl-item.z-component-swiper-contrl-item-active{
  background: #000;
  color:#fff;
  border-radius: 50rpx;
}
.z-component-swiper-play-icon{
  position: absolute;
  width:120rpx;
  height: 120rpx;
  background: url(data:image/png;base64,) no-repeat center;
  background-size: 100%;
  top:50%;
  left: 50%;
  margin-left: -60rpx;
  margin-top: -60rpx;
}
.z-component-swiper-top-contrl{
  color:#fff;
  font-size: 20rpx;
  position: absolute;
  top:0;
  left: 0;
  text-align: center;
  width:100%;
  white-space: nowrap;
}
.z-component-swiper-top-contrl-item{
  height: 60rpx;
  padding: 0 10rpx;
  line-height: 60rpx;
  display: inline-block;
  margin-right: 10rpx;
  margin-bottom: 10rpx;
}
.z-component-swiper-top-contrl-item:last-child{
  margin-right: 0;
}