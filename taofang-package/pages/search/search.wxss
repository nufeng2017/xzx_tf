.hui-search-container{
  padding: 30rpx;
  height: 72rpx;
  background: #f2f2f2;
}
.hui-search-box {
  height: 100%;
  background: #fff;
}
.hui-search-box input{
  font-size:28rpx;
}
.hui-search-box:before{
  content: '';
  display: block;
  height: 72rpx;
  width: 68rpx;
  background: url(data:image/png;base64,) no-repeat center;
  background-size:32rpx 32rpx;
}
.hui-search-list{
  padding: 0 30rpx;
}
.hui-search-list-item{
  color:#333;
  font-size:30rpx;
  padding: 30rpx 0;
  border-bottom: 1rpx solid #f5f5f5;
}
.hui-area-name{
  margin-bottom: 8rpx;
}