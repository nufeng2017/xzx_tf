page{
  background: #f5f5f5;
  padding-bottom: 120rpx;
}
.color-f87e44{
  padding: 22rpx 30rpx 22rpx 66rpx;
  background: #fefced;
  position: relative;
}
.color-f87e44:before{
  content: '';
  width:28rpx;
  height: 25rpx;
  background: url(data:image/png;base64,) no-repeat center;
  background-size:100% 100%;
  position: absolute;
  left: 30rpx;
  top:26rpx;
}
.hui-publish-type-wrap{
  background: #fff;
  padding: 0 30rpx;
  margin-bottom: 58rpx;
}
.hui-publish-type-wrap .hui-publish-item{
  height: 130rpx;
  border-bottom: 1px solid #f5f5f5;
  color:#000;
  font-size:30rpx;
}
.hui-publish-type-wrap .hui-publish-item:before{
  content: '';
  display: block;
  height: 100%;
  width:86rpx;
  background: url(data:image/png;base64,) no-repeat left center;
  margin-left: 3rpx;
  background-size:64rpx 62rpx;
}
.hui-publish-type-wrap .hui-publish-item:nth-child(2):before{
  background-image:url(data:image/png;base64,);
  background-size:62rpx 62rpx;
}
.hui-publish-type-wrap .hui-publish-item:nth-child(3):before{
  background-image:url(data:image/png;base64,);
  background-size:63rpx 59rpx;
}
.hui-publish-type-wrap .hui-publish-item:nth-child(4):before{
  background-image:url(data:image/png;base64,);
  background-size:54rpx 61rpx;
}
.hui-publish-type-wrap .hui-publish-item:nth-child(5):before{
  background-image:url(data:image/png;base64,);
  background-size:61rpx 58rpx;
}
.hui-publish-type-wrap .hui-publish-item:nth-child(6):before{
  background-image:url(data:image/png;base64,);
  background-size:55rpx 55rpx;
}
.hui-authentication{
  text-align: center;
}
.hui-authentication>view:first-child{
  margin-bottom: 24rpx;
}
.hui-authentication>view:first-child:before{
  display: inline-block;
  content: '';
  margin-right: 8rpx;
  width:24rpx;
  height: 24rpx;
  background: url(data:image/png;base64,) no-repeat center;
  background-size:100%;
  vertical-align: -10%;
}
.hui-authentication>view:last-child:after{
  content: '';
  display: inline-block;
  margin-left: 8rpx;
  background: url(data:image/png;base64,) no-repeat center;
  background-size:100%;
  width:9rpx;
  height: 17rpx;
  vertical-align: -1%;
}