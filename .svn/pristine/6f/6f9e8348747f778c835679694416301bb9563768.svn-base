page{
  height: 100%;
  background: #f5f5f5;
}
.hui-img-board{
  padding-left: 30rpx;
}
.hui-img-item{
  position: relative;
  margin-right: 40rpx;
  margin-bottom: 40rpx;
  width:200rpx;
  height: 200rpx;
}
.hui-img-item[data-show="true"] image{
  display: none;
}
.hui-img-item[data-show="true"]{
  background: url() no-repeat center;
  background-size:55rpx 55rpx;
  background-color: #eaeaea;
}
.hui-img-item[data-show="false"] image{
  display: block;
  width:100%;
  height: 100%;
  border-radius: 8rpx;
}