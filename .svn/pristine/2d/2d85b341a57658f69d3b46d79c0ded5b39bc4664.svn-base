/* pages/detailpages/detailpages.wxss */
page{
  padding-bottom: 100rpx;
}
/*轮播图容器*/
#imgBox{
  position: relative;
  margin-bottom:44rpx;
  height: 480rpx;
}
.overImg{
  position: absolute;
  top:0px;
  left:0px;
  height:100%;
  width:100%;
  z-index: 102;
}

#imgBox swiper{
  height: 100%;
  width:100%;
}
#imgBox image{
  width:100%;
  height: 480rpx;
}
#imgBox.shadow:after{
  content: '';
  display: block;
  position: absolute;
  bottom:0px;
  left:0px;
  width:100%;
  height:100rpx;
  background: -moz-linear-gradient(top, transparent,rgba(0, 0, 0, 0.2), rgba(0, 0, 0, 0.4));
  background: linear-gradient(top, transparent, rgba(0, 0, 0, 0.2),rgba(0, 0, 0, 0.4));
  background: -webkit-linear-gradient(top, transparent,rgba(0, 0, 0, 0.2), rgba(0, 0, 0, 0.4));
  background: -o-linear-gradient(top, transparent, rgba(0, 0, 0, 0.2),rgba(0, 0, 0, 0.4));
  z-index: 100;
}
#imgsNum{
  color:#fff;
  position: absolute;
  bottom: 15rpx;
  right: 35rpx;
  font-size:24rpx;
  z-index: 120;
}
/* 房源基本信息 */
.hui-content-box{
  padding: 0 38rpx;
}
.border-b{
  border-bottom:20rpx solid #f5f5f5;
}
.hui-details-title{
  line-height: 58rpx;
  font-size:38rpx;  
  position: relative;
  color:#242424;
  margin-bottom: 70rpx;
}
.hui-details-title:after{
  content:'';
  display: block;
  position: absolute;
  height: 32rpx;
  border-radius: 4rpx;
  color:#fff;
  font-size:24rpx;
  bottom:13rpx;
  right: 0rpx;
  line-height: 32rpx;
  padding: 0 6rpx;
}
/* 急租ICON */
.hui-details-title[data-icon="1"]:after{
  background: #fd4a4a;
  content: '\6025\79df';
}
/* 验真ICON */
.hui-details-title[data-icon="2"]:after{
  background: #58be4b;
  content: '\9a8c\771f';
}
.hui-details{
  text-align: center;
  margin-bottom: 70rpx;
  font-size:26rpx;
  color:#8a8a8a;
}
.hui-details .hui-font-color-y{
  color:#ff5500;
  margin-top: 20rpx;
}
.hui-details .flex-item{
  border-right:1rpx solid #e9e9e9;
}
.hui-details .flex-item:last-child{
  border-right:none;
}
.hui-details .hui-details-txt{
  font-size:36rpx;
  color:#ff5500;
}
.hui-characteristic{
  margin: 40rpx 0;
}
.hui-characteristic view{
  display: inline-block;
  font-size:24rpx;
  height: 35rpx;
  line-height: 35rpx;
  padding: 0 10rpx;
  margin-right: 20rpx;
}
.hui-characteristic view[ data-color="1"]{
  background: #ffe7db;
  color:#ff7a3b;
}
.hui-characteristic view[ data-color="2"]{
  background: #eff6fe;
  color:#448bc4;
}
.hui-characteristic view[ data-color="3"]{
  background: #e2f5e1;
  color:#36a333;
}
.hui-details-item{
  font-size:30rpx;
  color:#242424;
}
.hui-details-item .hui-font-color-g,.hui-details-wrap .hui-font-color-g{
  color:#8a8a8a;
  margin-right: 24rpx;
  white-space: nowrap;
}
.hui-details-item .flex-item>view{
  margin-bottom: 40rpx;
}
.hui-details-wrap{
  height: 108rpx;
  border-bottom:1rpx solid #ececec;
  line-height: 108rpx;
  font-size:30rpx;
  color:#242424;
}
.hui-details-wrap .hui-font-color-y{
  color:#ff9e4c;
}
.hui-details-module-title{
  font-size:36rpx;
  color:#242424;
  padding: 40rpx 0;
}
.hui-house-description{
  font-size:30rpx;
  color:#4a4a4a;
  line-height: 55rpx; 
  max-height: 385rpx; 
  overflow: hidden;
}

.otherDes{
  max-height: 380rpx;
  overflow: hidden;
}

.max-height{
  max-height:none;
  height: auto;
}
.hui-check-more{
  height: 80rpx;
  text-align: center;
}
.hui-check-more image{
  height: 12rpx;
  width:19rpx;
  display: inline-block;
}
.hui-facilities {
  font-size:24rpx;
  color:#404040;
  padding-bottom: 20rpx;
  
}
.hui-facilities .fl{
  width:20%;
  text-align: center;
  margin-bottom: 30rpx;
}
.hui-facilities image{
  width:60rpx;
  display: inline-block;
  height: 60rpx;
  margin-bottom: 20rpx;
}
.border-b-1{
  border-bottom: 1rpx solid #ececec;
}
.hui-report{
  height: 110rpx;
  font-size:30rpx;
  color:#404040;
}
.hui-report:before{
  content: '';
  display: block;
  width:46rpx;
  background: url(data:image/png;base64,) no-repeat left center;
  height: 36rpx;
  background-size:32rpx 36rpx;
}
.hui-report-tips{
  font-size:26rpx;
  color:#8a8a8a;
  margin-left: 13rpx;
}
.hui-report-icon{
  font-size:24rpx;
  color:#ff9e4c;
  width:72rpx;
  padding-left: 34rpx;
  position: relative;
  text-align: center;
}
.hui-report-icon::before{
  content: '';
  background: url(data:image/png;base64,) no-repeat center;
  display:inline-block;
  width:100%;
  height: 30rpx;
  background-size:28rpx 30rpx;
  margin-bottom: 4rpx;
}
.hui-report-icon:after{
  content:'';
  position: absolute;
  display: block;
  width:1rpx;
  height: 42rpx;
  background: #ececec;
  left: 0;
  top:15rpx;
}
.hui-map{
  position: relative;
  height: 290rpx;
  background: #ccc;
  margin-bottom: 40rpx;
}
.hui-map-icon{
  height: 45rpx;
  line-height: 45rpx;
  padding: 0 10rpx;
  color:#fff;
  font-size:26rpx;
  position: absolute;
  bottom:0;
  right: 0;
  background: rgba(0,0,0,0.6);
}
.hui-map image{
  width:100%;
  height: 100%;
}
.hui-map-icon:before{
  content:'';
  display: inline-block;
  margin-right: 5rpx;
  background: url(data:image/png;base64,) no-repeat center;
  width:23rpx;
  height: 21rpx;
  background-size:100% 100%;
}
.hui-periphery{
  color:#666;
  font-size:30rpx;
  line-height: 60rpx;
}
.hui-periphery text{
  color:#aaa;
}
.hui-periphery>view{
  margin-bottom: 10rpx;
  padding-left: 50rpx;
  position: relative;
}
.hui-periphery>view:before{
  content: '';
  display: block;
  position: absolute;
  width:33rpx;
  height: 34rpx;
  left: 0;
  background: url(data:image/png;base64,) no-repeat center;
  background-size:100%;
  top:13rpx;
}
.hui-periphery>view:nth-child(2):before{
  background-image:url(data:image/png;base64,);
  width:31rpx;
  height: 35rpx;

}
.hui-periphery>view:nth-child(3):before{
  background-image:url(data:image/png;base64,);
  width:31rpx;
  height: 31rpx;
  top:14rpx;
}
.hui-details-module-title text{
  font-size:30rpx;
  color:#8a8a8a;
  margin-left: 10rpx;
}
.hui-details-module-title-arrow{
  color:#8a8a8a;
  font-size:28rpx;
}
.hui-details-module-title-arrow:after{
  content:'';
  display: block;
  width:11rpx;
  height: 20rpx;
  background: url(data:image/png;base64,) no-repeat center;
  background-size:100% auto;
}
.hui-statement{
  padding: 30rpx 38rpx;
  font-size:18rpx;
  color:#999;
  background: #f5f5f5;
}
/* 底部按钮 */
.hui-bottom-info{
  position: fixed;
  padding-bottom: env(safe-area-inset-bottom);
  height: 100rpx;
  width:100%;
  bottom:0;
  left: 0;
}
.hui-bottom-info .flex-item:first-child{
  padding-left: 38rpx;
  background: #fff;
  box-sizing: border-box;
  position: relative;
}
.hui-bottom-info .flex-item:first-child .hui-border{
  display:block;
  position: absolute;
  top:0;
  left:0;
  width:100%;
  height: 1rpx;
  background: #ccc;
}
.hui-bottom-info .flex-item:first-child .hui-avatar{
  width:78rpx;
  height: 78rpx;
  border-radius: 50%;
  margin-right: 10rpx;
  overflow: hidden;
}
.hui-bottom-info .flex-item:first-child .hui-avatar cover-image{
  width:100%;
  height: auto;
  overflow: visible;
}
.hui-bottom-info .flex-item:last-child{
  background: #ffa229;
  line-height: 100rpx;
  color:#fff;
  font-size:32rpx;
  text-align: center;
}
.hui-bottom-info-addr{
  color:#8a8a8a;
  font-size:20rpx;
  margin-top: 8rpx;
}
.hui-bottom-info-name{
  color:#404040;
  font-size:28rpx;
}
.hui-bottom-info-name cover-image{
  display: inline-block;
  width: 20rpx;
  height: 20rpx;
  overflow: visible;
}
.hui-near-subway{
  align-items:flex-start;
}
.hui-near-subway-content{
  line-height: 1.5;
  min-height: 108rpx;
  height: auto;
}