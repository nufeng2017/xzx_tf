.hui-wrap{
  padding: 0 30rpx;
}
.hui-user-info{
  padding: 50rpx 0 80rpx;
}
.hui-user-info .hui-avatar{
  width:110rpx;
  height: 110rpx;
  border-radius: 50%;
  overflow: hidden;
  margin-right: 28rpx;
  font-size:32rpx;
}
.hui-user-info .hui-avatar image{
  width:100%;
  height: 100%;
}
.hui-authentication-icon{
  width:170rpx;
  height: 114rpx;
  background: url(data:image/png;base64,) no-repeat center;
  background-size:100% 100%;
}
.hui-authentication-icon.hui-no-authentication-icon{
  background-image:url(data:image/png;base64,);
}
.hui-authentication-pro {
  margin-bottom: 55rpx;
}
.hui-authentication-pro .hui-authentication-item{
  border-radius: 6rpx;
  background: #f6f7f9;
  height: 158rpx;
  width:100%;
  color:#404040;
  font-size:30rpx;
  margin-bottom: 30rpx;
  box-sizing: border-box;
  padding: 0 40rpx;
}
.hui-authentication-pro .hui-authentication-item .hui-authentication-item-title{
  margin-bottom: 8rpx;
}
.hui-authentication-pro .hui-authentication-item:before{
  content: '';
  margin-right: 28rpx;
  display: block;
  width:80rpx;
  height: 80rpx;
  background:url(data:image/png;base64,) no-repeat center;
  background-size:100% 100%;
}
.hui-authentication-pro .hui-authentication-item.hui-bank:before{
  background-image: url(data:image/png;base64,);
}
.hui-authentication-pro .hui-authentication-item:after{
  content: '';
  display: block;
  height: 100%;
  background: url(data:image/png;base64,) no-repeat center;
  width:16rpx;
  background-size:100% auto;
}
.yellow-font{
  color:#ff7500 !important;
}
/* 实名认证优势 */
.hui-advantage-header{
  color:#404040;
  text-align: center;
  padding-bottom: 25rpx;
  font-size:30rpx;
  border-bottom: 1rpx solid #f1f1f1;
  margin-bottom: 20rpx;
}
.hui-advantage .hui-advantage-main .hui-advantage-item{
  text-align: center;
  width:50%;
  padding: 38rpx 0;
  line-height: 1;
  box-sizing: border-box;
  border-bottom: 1rpx solid #f1f1f1;
}
.hui-advantage .hui-advantage-main .hui-advantage-item:nth-child(3),.hui-advantage .hui-advantage-main .hui-advantage-item:nth-child(4){
  border-bottom:none;
}
.hui-advantage .hui-advantage-main .hui-advantage-item:nth-child(2n-1){
  border-right: 1rpx solid #f1f1f1;
}
.hui-advantage .hui-advantage-main .hui-advantage-item:before{
  content: '';
  display: inline-block;
  width:61rpx;
  height: 48rpx;
  margin-bottom: 24rpx;
  background: url(data:image/png;base64,) no-repeat center;
  background-size:100% 100%;
}
.hui-advantage .hui-advantage-main .hui-advantage-item .hui-advantage-item-title{
  margin-bottom: 9rpx;
  font-size:26rpx;
  color:#404040;
}
.hui-advantage .hui-advantage-main .hui-advantage-item:nth-child(2):before{
  background-image:url(data:image/png;base64,);
  width:45rpx;
  height: 54rpx;
}
.hui-advantage .hui-advantage-main .hui-advantage-item:nth-child(3):before{
  background-image:url(data:image/png;base64,);
  width:53rpx;
  height: 52rpx;
}
.hui-advantage .hui-advantage-main .hui-advantage-item:nth-child(4):before{
  background-image:url(data:image/png;base64,);
  width:52rpx;
  height: 46rpx;
}