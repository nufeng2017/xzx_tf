.z-component-brand-presence{
  position: fixed;
  width:150rpx;
  height: 66rpx;
  box-sizing: border-box;
  bottom: 165rpx;
  right: 0;
  padding-left: 70rpx;
  line-height: 1.2;
  font-size: 22rpx;
  color:#000;
  border-top-left-radius: 50rpx;
  border-bottom-left-radius: 50rpx;
  box-shadow: 0rpx 0 8rpx 2rpx rgba(0,0,0,0.3);
  background: #fff;
}
.z-component-brand-presence .z-component-brand-presence-image{
  width:52rpx;
  height: 52rpx;
  position: absolute;
  top:7rpx;
  left: 7rpx;
}
.z-component-brand-presence-input{
  height: 100rpx;
  margin: 0 44rpx;
  border-bottom: 1px solid #e6e6e6;
  position: relative;
}
.z-component-brand-presence-input .z-component-brand-presence-input-c{
  width:100%;
  height:100%;
  border:none;
  outline:none;
  background: transparent;
  color:#000;
  font-size: 30rpx;
}
.z-component-brand-presence-button{
  margin: 50rpx 44rpx;
}
count-down{
  position: absolute;
  top:19rpx;
  right: 0;
}
.z-component-brand-presence-count-down{
  padding-right: 182rpx;
}