page{
    background: #f4f4f4;
    font-size: 30rpx;
    padding-bottom:98rpx;
}
.house_part{
    background: #fff;
    border-bottom: 1rpx solid #e5e5e5;
    padding: 30rpx 0 30rpx 30rpx;
    height: auto;
    overflow: hidden;
    margin-bottom: 30rpx;
}
.house_part > view{
    float: left;
}
.house_part > view:first-child{
    margin-right: 34rpx;
    width: 210rpx;
    height: 160rpx;
}
.house_part > view:first-child image{
    width: 100%;
    height: 100%;
}
.house_part > view:last-child{
    width: 430rpx;
    position: relative;
}
.house_part > view:last-child text{
    display: block;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
    height: 53rpx;
    line-height: 53rpx;
}
.house_part > view:last-child > text:first-child{
    font-size: 30rpx;
    color: #333;
    font-weight: bolder;
}
.house_part > view:last-child > text:nth-child(2){
    font-size: 22rpx;
    color: #656565;
}
.house_part > view:last-child view.dianzi{
    line-height: 30rpx;
    height: 30rpx;
    text-align: center;
    font-size: 24rpx;
    display: inline-block;
    border-radius: 4rpx;
    color: #ff0600;
}
.house_part > view:last-child view.dianzi view:first-child{
    color: #7e97f2;
    width: 110rpx;
    background: #f2f0ff;
    margin-right: 10rpx;
}
.house_part > view:last-child view.paper{
    line-height: 30rpx;
    height: 30rpx;
    text-align: center;
    font-size: 24rpx;
    display: inline-block;
    border-radius: 4rpx;
    color: #ff0600;
}
.house_part > view:last-child view.paper view:first-child{
    color: #31acd5;
    width: 110rpx;
    background: #e7faff;
    margin-right: 10rpx;
}
.house_part > view:last-child view view{
    display: inline-block;
}
.house_part > view:last-child text text{
    margin-right: 10rpx;
    display: inline-block;
}
.house_part > view:last-child view.seecontract{
    position: absolute;
    right: 0;
    bottom: -13rpx;
    background: #f8f8f8;
    border: 2rpx solid #dfdfdf;
    border-radius: 4rpx;
    color: #444;
    font-size: 24rpx;
    width: 120rpx;
    height: 56rpx;
    text-align: center;
    line-height: 56rpx;
}
.house_part > view:last-child .rental{
  position: absolute;
  right: 0;
  top:50rpx;
  color: #ffa229;
  font-size: 30rpx;
  margin-right: 0;
}
.house_part > view:last-child .rental text{
  font-size: 22rpx;
  display: inline;
  margin-right: 0;
}

.main {
    background: #fff;
    margin-bottom: 30rpx;
    overflow: hidden;
    padding: 0 30rpx 0 40rpx;
}
.main view.main_title{
    color: #333;
    font-size: 30rpx;
    line-height: 48rpx;
    margin-top: 28rpx;
    margin-bottom: 30rpx;
    font-weight: bold;
}
.main view.main_con{
    overflow: hidden;
    margin-bottom: 30rpx;
}
.main view.main_con view:first-child{
    float: left;
    font-size: 28rpx;
    color: #4c4c4c;
}
.main view.main_con view:last-child{
    float: right;
    font-size: 30rpx;
    color: #333;
}
.main .hetong_img {
    overflow: hidden;
}
.main .hetong_img image {
    float: left;
    width: 160rpx;
    height: 160rpx;
    display: inline-block;
    margin-right: 40rpx;
    margin-bottom: 40rpx;
}
.main.lastmain{
    margin-bottom: 120rpx;
}
.footer {
    position: fixed;
    bottom: 0;
    left: 0;
    width: 100%;
    height: 100rpx;
    z-index: 1000;
    background: #fff;
    /* border-top: 1rpx solid #cccccc; */
    display: flex;
}
.footer > view{
    flex: 1;
    height: 100%;
    text-align: center;
    line-height: 100rpx;
    color: #fff;
    font-size: 32rpx;
    background: #ff6f29;
}
.footer view.fl:first-child {
    background: #ffa229;
}
.footer .fl:first-child {
    background: #ffa229;
}
.seeAllpaid {
    width: 100%;
    font-size: 24rpx;
    color: #4b96ff;
    text-align: center;
    line-height: 80rpx;
}
.seeAllpaid image{
    width: 20rpx;
    height: 12rpx;
    padding-left:12rpx;
}
.history{
    border-bottom: 1px dashed #999;
    padding: 0.75rem 0 0 0;
}
.main .history:last-child {
    border-bottom: none;
}