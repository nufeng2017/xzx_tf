page,scroll-view{
  height: 100%;
}
.hui-container{
  padding: 40rpx 0 170rpx;
}
.hui-pb{
  padding-bottom: 42rpx !important;
}
.hui-wrap{
  padding: 0 30rpx 70rpx;
}
.hui-block-title{
  line-height: 1;
  font-size:32rpx;
  color:#333;
  margin-bottom: 20rpx;
}
.hui-block-item{
  min-width:162rpx;
  box-sizing:border-box;
  height:64rpx;
  margin:0 30rpx 20rpx 0;
  border:1rpx solid #dfdfdf;
  line-height:62rpx;
  background:#fcfcfc;
  color:#333;
  text-align:center;
  font-size:28rpx;
  border-radius:10rpx;
  padding: 0 10rpx;
}
.hui-block-item.hui-active{
  background:#ffeacd;
  border-color:#ffb657;
}
.hui-sel-area{
  height: 80rpx;
  border-radius: 10rpx;
  border:1rpx solid #e6e6e6;
  position: relative;
  padding: 0 80rpx 0 50rpx;
  line-height: 80rpx;
  color:#cccccc;
  font-size:30rpx;
  white-space: nowrap;
  overflow: hidden
}
.hui-sel-area:before{
  content: '';
  display: block;
  position: absolute;
  width:50rpx;
  height: 100%;
  top:0;
  left: 0;
  background: url(data:image/png;base64,) no-repeat center;
 background-size:25rpx 30rpx; 
}
.hui-sel-area:after{
  content: '';
  display: block;
  position: absolute;
  width:80rpx;
  height: 100%;
  top:0;
  right:0;
  background: url(data:image/png;base64,) no-repeat center;
 background-size:21rpx 11rpx; 
 background-color:#fff;
}
.hui-house-description {
  padding:20rpx 30rpx 55rpx;
  position:relative;
  border-radius:10rpx;
  border:1rpx solid #e6e6e6;
  box-sizing:border-box;
  min-height:242rpx;
  width:100%;
}
.hui-house-description .hui-textare{
  min-height:187rpx;
  width:100%;
  font-size:30rpx;
  color:#ccc;
  word-break:break-all;
}
.hui-textarea-txt-num {
  position:absolute;
  font-size:24rpx;
  color:#ccc;
  right:30rpx;
  bottom:14rpx;
}
.hui-form .hui-form-item{
  height: 124rpx;
  border-bottom: 1rpx solid #e6e6e6;
}
.hui-form .hui-form-item input{
  height: 100%;
  font-size:32rpx;
}
.hui-sex-sel{
  width:258rpx;
  border-radius: 50rpx;
  overflow: hidden;
  height: 64rpx;
  line-height: 64rpx;
  background: #f2f2f2;
  text-align: center;
  color:#333;
  font-size:28rpx;
}
.hui-sex-sel>view{
  width:50%;
}
.hui-sex-sel>view.hui-sel-this{
  color:#fff;
  background: #ffa229;
}
.bottom-border-none{
  border-bottom: none;
}
/* 弹窗 */
.hui-model-box{
  position: fixed;
  z-index: 10000;
  top:0;
  left: 0;
  width:100%;
  height: 100%;
}
.hui-model-container{
  width:560rpx;
  box-sizing: border-box;
  height: 490rpx;
  position: relative;
  padding: 0 52rpx 106rpx;
  border-radius: 10rpx;
  background: #fff;
  overflow: hidden;
}
.hui-popup-input{
  border-radius: 8rpx;
  background: #f2f2f2;
  border:1rpx solid #e0e0e0;
  overflow: hidden;
  height: 90rpx;
  padding: 0 26rpx;
  margin-bottom: 24rpx;
  position: relative;
}
.hui-popup-input input{
  font-size:30rpx;
  color:#333;
  width:100%;
  height: 100%;
}
.hui-code{
  position: relative;
  border-radius: 10rpx;
  overflow: hidden;
}
.hui-code .hui-popup-input{
  padding-right: 150rpx;
  margin-bottom: 0rpx;
}
.hui-get-code-btn{
  width:130rpx;
  position: absolute;
  right: 0rpx;
  top:0rpx;
  height: 92rpx;
  background: #ffa229;
  color:#fff;
  font-size:30rpx;
  line-height: 90rpx;
  text-align: center;
}
.hui-popup-title{
  text-align: center;
  padding: 52rpx 0 40rpx;
}
.hui-popup-btn{
  position: absolute;
  height: 105rpx;
  line-height: 105rpx;
  width:100%;
  bottom:0;
  left:0;
  border-top:1rpx solid #d2d3d5;
  text-align: center;
  color:#333;
  font-size:36rpx;
}
.hui-popup-btn view{
  width:50%;
  box-sizing: border-box;
  height: 100%;
}
.hui-popup-btn-ok{
  color:#ff6f29;
  border-left: 1rpx solid #d2d3d5;
}
/* 底部按钮 */
.hui-submit-btn{
  position: fixed;
  width:100%;
  font-size:36rpx;
  color:#fff;
  text-align: center;
  height: 100rpx;
  line-height: 100rpx;
  background: #ffa229; 
  bottom:0;
  left: 0;
  z-index: 1000;
}
.hui-txt-tips-1{
  font-size:24rpx;
  padding-bottom:100rpx;
  color:#9aa4b3;
  margin: 0 30rpx;
  padding-left: 35rpx;
  position: relative;
}
.hui-txt-tips-1 image{
  position: absolute;
  width:24rpx;
  height: 24rpx;
  left: 0;
  top:4rpx;
}
/* 筛选 弹窗 */
.hui-screen-area-box{
  position: fixed;
  z-index: 10000;
  top:0;
  left: 0;
  width:100%;
  min-height: 930rpx;
  background: #fff;
}
.hui-screen-board{
  height: 780rpx;
  box-sizing: border-box;
  border:1rpx solid #e5e5e5;
  border-left: none;
  border-right: none;
  margin-bottom: 30rpx;
}
.hui-screen-board>view{
  height: 100%;
  overflow-y:auto;
  color:#333;
  font-size:30rpx; 
}
.hui-screen-board>view>view{
  height: 90rpx;
  line-height: 90rpx;
  padding:0 10rpx 0 48rpx;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
  word-break: normal;
}
.hui-screen-board>view .area-active{
  color:#ffa229;
  background: #fff;
}
.hui-screen-board>view:nth-child(1){
  background: #e6e6e6;
  width:180rpx;
}
.hui-screen-board>view:nth-child(2){
  width:240rpx;
  border-right: 1rpx solid #e5e5e5;
}
.hui-already-sel-show{
  position: relative;
  padding-left: 88rpx;
  color:#999;
  font-size:24rpx;
  line-height: 44rpx;
}
.hui-already-sel-show>text{
  position: absolute;
  top:0;
  left: 0;
  width:88rpx;
  text-align: center;
}
.hui-already-sel-show .hui-delete-btn{
  width:40rpx;
  height: 100%;
  background: url(data:image/png;base64,) no-repeat center;
  background-size:13rpx 13rpx;
  display: inline-block;
  vertical-align: top;
}
.hui-already-sel-show .fl{
  height: 44rpx;
  margin-bottom: 26rpx;
  padding: 0 0rpx 0 13rpx;
  font-size:20rpx;
  color:#ffa229;
  background: #fff3e2;
  margin-right: 30rpx;
}
.hui-already-sel-show .fl text{
  line-height: 44rpx;
  display: inline-block;
  vertical-align: top;
  height: 100%;
}
.hui-screen-btn-group{
  padding-bottom: 40rpx;
  font-size:30rpx;
  line-height: 80rpx; 
}
.hui-screen-btn-group>view{
  text-align: center;
  width:340rpx;
  height: 80rpx;
  border-radius: 6rpx;
  color:#999999;
  background: #f2f2f2;
  margin-right: 28rpx;
}
.hui-screen-btn-group>view.hui-determine-btn{
  background: #ffa229;
  color:#fff;
  margin-left: 0rpx;
}
.hui-f3{
  background: #f3f3f3 !important;
}