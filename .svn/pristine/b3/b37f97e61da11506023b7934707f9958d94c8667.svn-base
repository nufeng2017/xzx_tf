page {
  background: #f5f5f5;
}

.hui-page-tips {
  background: #f5f5f5;
  padding-top: 18rpx;
  height: 64rpx;
  padding-right: 30rpx;
}

.hui-more-service {
  height: 20rpx;
  border-right: 1rpx solid #9aa4b3;
  padding-right: 16rpx;
  margin-right: 16rpx;
  position: relative;
  line-height: 1;
}

.hui-more-service:before {
  content: '';
  display: block;
  position: absolute;
  width: 42rpx;
  height: 22rpx;
  background: url(data:image/png;base64,) no-repeat center;
  background-size: 100% 100%;
  top: -24rpx;
  right: 20rpx;
}

.hui-publish-wrap {
  padding-bottom: 80rpx;
  background: #f5f5f5;
}

.hui-publish-item {
  margin-bottom: 20rpx;
  background: #fff;
}

.hui-publish-item .hui-publish-details {
  position: relative;
  padding: 36rpx 0 36rpx 188rpx;
  margin: 0 30rpx;
}

.hui-publish-item .hui-publish-img {
  position: absolute;
  height: 130rpx;
  width: 161rpx;
  overflow: hidden;
  top: 36rpx;
  left: 0;
}

.hui-publish-item .hui-publish-img image {
  width: 100%;
  height: 100%;
}

.hui-publish-item .hui-publish-info {
  line-height: 1;
  height: 130rpx;
  width: 100%;
}

.hui-publish-item .hui-price {
  font-size: 18rpx;
  color: #ccc;
}

.hui-publish-btn-group {
  text-align: center;
  width: 100%;
  border-top: 1rpx solid #f2f2f2;
  color: #666;
  font-size: 28rpx;
  height: 98rpx;
  line-height: 98rpx;
}

.hui-publish-btn-group>view:before {
  content: '';
  display: inline-block;
  vertical-align: top;
  height: 100%;
  width: 30rpx;
  margin-right: 10rpx;
  background: url(data:image/png;base64,) no-repeat center;
  background-size: 100%;
}

.hui-publish-btn-group>view.hui-delete-publish:before {
  background-image: url(data:image/png;base64,);
}

.hui-publish-btn-group>view.hui-refresh:before {
  background-image: url(data:image/png;base64,);
}

.hui-publish-btn-group>view.hui-change:before {
  background-image: url(data:image/png;base64,);
}

.hui-publish-btn-group>view.hui-share:before {
  background-image: url(data:image/png;base64,);
}

.hui-publish-btn-group>view.hui-phone:before {
  background-image: url(data:image/png;base64,);
}

.hui-publish-btn-group>view.hui-phone {
  padding-left: 30rpx;
  width: 582rpx;
  box-sizing: border-box;
  text-align: left;
  font-size: 22rpx;
}

.hui-not-pass .hui-delete-publish {
  text-align: left;
}

/* 已失效 */

.hui-invalid .hui-publish-img:before, .hui-review .hui-publish-img:before,
.hui-not-pass .hui-publish-img:before {
  content: '\5df2\5931\6548';
  display: block;
  position: absolute;
  top: 0;
  left: 0;
  background: #dfdfdf;
  color: #9f9f9f;
  font-size: 20rpx;
  width: 80rpx;
  height: 36rpx;
  text-align: center;
  line-height: 36rpx;
  border-bottom-right-radius: 10rpx;
}

/* 审核中 */

.hui-review .hui-publish-img:before {
  content: '\5ba1\6838\4e2d';
  color: #ff8928;
  background: rgba(0, 0, 0, 0.8);
}

/* 未通过 */

.hui-not-pass .hui-publish-img:before {
  content: '\672a\901a\8fc7';
  color: #ffeded;
  background: rgba(210, 0, 0, 0.8);
}

