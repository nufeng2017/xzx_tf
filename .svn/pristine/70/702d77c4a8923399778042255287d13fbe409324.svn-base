.hui-promise-item{
  padding-left: 126rpx;
  margin-bottom: 75rpx;
}
.hui-promise-item:before{
  content: '';
  display: inline-block;
  width:66rpx;
  height:66rpx;
  background: url(data:image/png;base64,) no-repeat center;
  background-size:66rpx 51rpx;
  margin-right: 16rpx;
}
.hui-promise-item:nth-child(2):before{
  background-image:url(data:image/png;base64,);
  background-size:63rpx 57rpx;
}
.hui-promise-item:nth-child(3):before{
  background-image:url(data:image/png;base64,);
  background-size:66rpx 60rpx;
}
.hui-promise-item:nth-child(4):before{
  background-image:url(data:image/png;base64,);
  background-size:65rpx 61rpx;
}

.hui-publish-btn{
  width:92%;
  font-size:36rpx;
  margin:0 auto 30rpx;
  color:#fff;
  background: #ffa229;
}