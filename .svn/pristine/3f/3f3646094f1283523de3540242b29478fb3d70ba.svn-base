page {
  background: #f5f5f5;
}

.hui-page-tips {
  background: #f5f5f5;
  padding-top: 18rpx;
  height: 64rpx;
  padding-right: 30rpx;
}

.hui-more-service {
  height: 20rpx;
  padding-right: 16rpx;
  margin-right: 16rpx;
  position: relative;
  line-height: 1;
}

.right-line{
 border-right: 1rpx solid #9aa4b3;
}

.hui-more-service:before {
  content: '';
  display: block;
  position: absolute;
  width: 42rpx;
  height: 22rpx;
  background: url(data:image/png;base64,) no-repeat center;
  background-size: 100% 100%;
  top: -24rpx;
  right: 20rpx;
}

.hui-publish-wrap {
  padding-bottom: 80rpx;
  background: #f5f5f5;
}

.hui-publish-item {
  margin-bottom: 20rpx;
  background: #fff;
}

.hui-publish-item .hui-publish-details {
  position: relative;
  padding: 36rpx 0 36rpx 188rpx;
  margin: 0 30rpx;
}

.hui-publish-item .hui-publish-img {
  position: absolute;
  height: 130rpx;
  width: 161rpx;
  overflow: hidden;
  top: 36rpx;
  left: 0;
}

.hui-publish-item .hui-publish-img image {
  width: 100%;
  height: 100%;
}

.hui-publish-item .hui-publish-info {
  line-height: 1;
  height: 130rpx;
  width: 100%;
}

.hui-publish-item .hui-price {
  font-size: 18rpx;
  color: #ccc;
}

.hui-publish-btn-group {
  text-align: center;
  width: 100%;
  border-top: 1rpx solid #f2f2f2;
  color: #666;
  font-size: 28rpx;
  height: 98rpx;
  line-height: 98rpx;
}

.hui-publish-btn-group>view:before {
  content: '';
  display: inline-block;
  vertical-align: top;
  height: 100%;
  width: 30rpx;
  margin-right: 10rpx;
  background: url(data:image/png;base64,) no-repeat center;
  background-size: 100% auto;
}

.hui-publish-btn-group>view.hui-delete-publish:before {
  background-image: url(data:image/png;base64,);
}

.hui-publish-btn-group>view.hui-refresh:before {
  background-image: url(data:image/png;base64,);
}

.hui-publish-btn-group>view.hui-change:before {
  background-image: url(data:image/png;base64,);
}
.hui-publish-btn-group>view.hui-share button{
  display: inline-block;
  padding: 0;
  height: 100%;
  font-size:28rpx;
  background: #fff;
  border:none;
  outline: none;
  color:#666;
  line-height: 98rpx;
}
.hui-publish-btn-group>view.hui-share button:before,.hui-publish-btn-group>view.hui-share button:after{
  border: none;
}
.hui-publish-btn-group>view.hui-share:before {
  background-image: url(data:image/png;base64,);
}

.hui-publish-btn-group>view.hui-phone:before {
  background-image: url(data:image/png;base64,);
}

.hui-publish-btn-group>view.hui-phone {
  padding-left: 30rpx;
  width: 582rpx;
  box-sizing: border-box;
  text-align: left;
  font-size: 22rpx;
}

.hui-not-pass .hui-delete-publish {
  text-align: left;
}

/* 已失效 */

.hui-invalid .hui-publish-img:before, .hui-review .hui-publish-img:before,
.hui-not-pass .hui-publish-img:before {
  content: '\5df2\5931\6548';
  display: block;
  position: absolute;
  top: 0;
  left: 0;
  background: #dfdfdf;
  color: #9f9f9f;
  font-size: 20rpx;
  width: 80rpx;
  height: 36rpx;
  text-align: center;
  line-height: 36rpx;
  border-bottom-right-radius: 10rpx;
}

/* 审核中 */

.hui-review .hui-publish-img:before {
  content: '\5ba1\6838\4e2d';
  color: #ff8928;
  background: rgba(0, 0, 0, 0.8);
}

/* 未通过 */

.hui-not-pass .hui-publish-img:before {
  content: '\672a\901a\8fc7';
  color: #ffeded;
  background: rgba(210, 0, 0, 0.8);
}


.left-margin-20{
  margin-left: 20rpx
}
.not-found-1{
  height: 100%;
  background: f5f5f5;
  width:100%;
  position: fixed;
  z-index: 10000;
  top:64rpx;
  left: 0;
  
}
.not-found-1 image{
  width:358rpx;
  height: 372rpx;
  margin: 160rpx auto 0 ;
  display: block;
}
.not-found-1 view{
  font-size:36rpx;
  color:#fff;
  text-align: center;
  line-height: 100rpx;
  background: #ffa229;
  border-radius: 6rpx;
  width:350rpx;
  margin: 40rpx auto 0;
}
/* tab按钮 */
.hui-tab-group{
  margin-bottom: 20rpx;
  color:#333333;
  font-size:30rpx;
  text-align: center;
  padding: 0 30rpx;
  background: #fff
}
.hui-tab{
  height: 100rpx;
  line-height: 100rpx;
  position: relative;
}
.hui-tab.hui-tab-active{
  color:#ffa229;
}
.hui-tab.hui-tab-active:after{
  content: '';
  display: block;
  position: absolute;
  bottom:0;
  left:50%;
  margin-left: -77rpx;
  width:154rpx;
  height: 6rpx;
  background: #ffa229;
}
/* 求租 */
.hui-house-info-item{
  margin-bottom: 20rpx;
  background: #fff;
  color:#999;
  font-size:24rpx;
  line-height: 1;
}
.hui-house-info-title{
  font-size:32rpx;
  color:#282828;
  padding:38rpx 30rpx 24rpx;
}
.hui-house-info{
  padding: 0rpx 30rpx 15rpx;
  border-bottom: 1rpx solid #f2f2f2;
}
.hui-house-info-box{
  margin-bottom: 14rpx;
  padding-left: 124rpx;
  position: relative;
  min-height: 24rpx;
}
.hui-house-info-box text{
  position: absolute;
  display: block;
  width:124rpx;
  left: 0;
  top:0;  
  text-align: justify;
}
.hui-house-info-specific{
  color:#666666;
}
.hui-house-info-specific>view{
  display: inline-block;
  vertical-align: top;
  font-size:20rpx;
  color:#ffa229;
  background: #fff3e2;
  height: 30rpx;
  line-height: 30rpx;
  padding: 0 13rpx;
  margin-right: 8rpx;
}
.hui-change-time{
  height: 88rpx;
  line-height: 88rpx;
  font-size:22rpx;
  padding: 0 30rpx;
}
.hui-change-time .hui-delete-btn{
  color:#666;
  font-size:26rpx;
}
.hui-change-time .hui-delete-btn image{
  width:30rpx;
  height: 30rpx;
  margin-right: 10rpx;
  display: inline-block;
  vertical-align: -4rpx;
}