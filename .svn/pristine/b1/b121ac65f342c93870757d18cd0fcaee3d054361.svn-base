.hui-flex-align-center{
	align-items: center;
	-moz-align-items: center;
	-o-align-items: center;
	-ms-align-items: center;
	box-align: center;
	-webkit-box-align: center;
	-o-box-align: center;
	-moz-box-align: center;
}
.hide{
  display: none !important;
}
.ad-f{
	width:17%;
	position: fixed;
	bottom:16%;
	right:20rpx;
}
/*兼容iphonex底部黑条  */
.iphonex{
    bottom:64rpx!important;
}
.iphonex::after{
    content: '';
    width: 100%;
    height: 64rpx;
    background: #fff;
    z-index: 999;
    position: fixed;
    bottom: 0;
    left: 0;
}
/*弹窗  */
.popup-bg{
  position: fixed;
  height:100%;
  width:100%;
  background: rgba(0,0,0,0.4);
  top:0;
  left:0;
  z-index: 100000;
}
.popup-none{
  display: none !important;
}
/*电话 弹窗  */
#check-phone .popup-container{
  background-color:#fff;
  border-radius: 8rpx;
  height:400rpx;
  width:600rpx;
  text-align: center;
  position: relative;
}
#check-phone .popup-title{
  font-size:36rpx;
  padding-top: 50rpx;
  margin-bottom: 22rpx;
}
#check-phone .phone-num{
  font-size:48rpx;
}

#check-phone .phone-num text{
  color:#ff5501;
}
#check-phone .oter-phone-num{
  font-size:26rpx;
  margin-top: 14rpx;
  color:#999;
}
#check-phone .cl{
  position: absolute;
  width:100%;
  height:108rpx;
  border-top:1rpx solid #d1d1d1;
  left:0;
  bottom:0;
  line-height: 108rpx
}
#check-phone .cl:before{
  content: '';
  position: absolute;
  width:2rpx;
  height:108rpx;
  top:0;
  left:50%;
  margin-left: -1rpx;
  background:#d1d1d1;
}
#check-phone .cl>text{
  width:50%;
  color:#666;
  font-size:36rpx;
}
#check-phone .phone-ok{
  color:#35589c !important;
}