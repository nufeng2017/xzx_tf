.hui-wrap{
  padding-bottom: 100rpx;
}
.hui-house-info-wrap{
  padding: 35rpx 0rpx 39rpx;
  margin: 0 30rpx;
  border-bottom:1rpx solid #f5f5f5;
}
.hui-house-info-wrap .dark-font{
  margin-bottom: 15rpx;
}
.hui-house-base-info input{
  font-size:32rpx;
  color:#333;
}
.hui-house-equipment .hui-equipment-item{
  width:162rpx;
  box-sizing: border-box;
  height: 64rpx;
  margin:0 14rpx 28rpx 0;
  border:1rpx solid #dfdfdf;
  line-height: 62rpx;
  background: #fcfcfc;
  color:#333;
  text-align: center;
  font-size:28rpx;
  border-radius: 10rpx;
}
.hui-house-equipment .hui-equipment-item:nth-child(4n){
  margin-right: 0rpx;
}
.hui-house-equipment-wrap{
  padding-bottom: 11rpx;
}
.hui-house-equipment .hui-equipment-item.hui-has-this{
  background: #ffeacd;
  border-color:#ffb657;
}
.hui-house-description{
  padding: 20rpx 30rpx 55rpx;
  position: relative;
  border-radius: 10rpx;
  border:1rpx solid #e6e6e6;
  box-sizing: border-box;
  min-height: 242rpx;
  width:100%;
}
.hui-house-description .hui-textarea{
  min-height: 187rpx;
  width: 100%;
  font-size:24rpx;
  color:#ccc;
}
.hui-textarea-txt-num{
  position: absolute;
  font-size:24rpx;
  color:#ccc;
  right: 30rpx;
  bottom: 14rpx;
}
.hui-form .hui-form-item{
  margin: 0 30rpx;
  height: 124rpx;
  border-bottom: 1rpx solid #f5f5f5;
}
.hui-form .hui-form-item input{
  height: 100%;
  font-size:32rpx;
}
.hui-sex-sel{
  width:258rpx;
  border-radius: 50rpx;
  overflow: hidden;
  height: 64rpx;
  line-height: 64rpx;
  background: #f2f2f2;
  text-align: center;
  color:#333;
  font-size:28rpx;
}
.hui-sex-sel>view{
  width:50%;
}
.hui-sex-sel>view.hui-sel-this{
  color:#fff;
  background: #ffa229;
}
.bottom-border-none{
  border-bottom: none;
}
/* 发布成功提示 */
.hui-submit-tips{
  position: fixed;
  top:0;
  left: 0;
  width:100%;
  height: 68rpx;
  background:#00bd71;
  color:#fff;
  font-size:24rpx;
  z-index: 2000;
}
.hui-submit-tips:before{
  content: '';
  display: block;
  width:31rpx;
  height: 31rpx;
  background: url(data:image/png;base64,) no-repeat center;
  background-size:100%;
  margin-right: 13rpx;
}
/* 弹窗 */
.hui-model-box{
  position: fixed;
  z-index: 10000;
  top:0;
  left: 0;
  width:100%;
  height: 100%;
}
.hui-model-container{
  width:560rpx;
  box-sizing: border-box;
  height: 490rpx;
  position: relative;
  padding: 0 52rpx 106rpx;
  border-radius: 10rpx;
  background: #fff;
  overflow: hidden;
}
.hui-popup-input{
  border-radius: 8rpx;
  background: #f2f2f2;
  border:1rpx solid #e0e0e0;
  overflow: hidden;
  height: 90rpx;
  padding: 0 26rpx;
  margin-bottom: 24rpx;
  position: relative;
}
.hui-popup-input input{
  font-size:30rpx;
  color:#333;
  width:100%;
  height: 100%;
}
.hui-code{
  position: relative;
  border-radius: 10rpx;
  overflow: hidden;
}
.hui-code .hui-popup-input{
  padding-right: 150rpx;
  margin-bottom: 0rpx;
}
.hui-get-code-btn{
  width:130rpx;
  position: absolute;
  right: 0rpx;
  top:0rpx;
  height: 92rpx;
  background: #ffa229;
  color:#fff;
  font-size:30rpx;
  line-height: 90rpx;
  text-align: center;
}
.hui-popup-title{
  text-align: center;
  padding: 52rpx 0 40rpx;
}
.hui-popup-btn{
  position: absolute;
  height: 105rpx;
  line-height: 105rpx;
  width:100%;
  bottom:0;
  left:0;
  border-top:1rpx solid #d2d3d5;
  text-align: center;
  color:#333;
  font-size:36rpx;
}
.hui-popup-btn view{
  width:50%;
  box-sizing: border-box;
  height: 100%;
}
.hui-popup-btn-ok{
  color:#ff6f29;
  border-left: 1rpx solid #d2d3d5;
}