page{
  height: 100%;
  overflow: hidden;
}
scroll-view{
  height: 100%;
}
.hui-tips{
  color:#fe9e15;
  background: #302f38;
  height: 60rpx;
  line-height: 60rpx;
  text-align: center;
}
.hui-upload-img-bg{
  width: 100%;
  height: 442rpx;
  position: relative;
  overflow: hidden;
}
.hui-upload-img-bg image{
  width: 100%;
}
.hui-upload-img-btn-1{
  width:202rpx;
  height: 202rpx;
  border-radius: 50%;
  border:4rpx solid #c6cad3;
  background: rgba(10,14,21,0.6);
  color:#fff;
  font-size:28rpx;
  position: absolute;
  left:50%;
  top:50%;
  margin-left: -105rpx;
  margin-top: -105rpx;
  z-index: 100;
}
.hui-upload-img-btn-1:before{
  content: '';
  display: block;
  background:url(data:image/png;base64,) no-repeat center;
  width:57rpx;
  height: 48rpx;
  background-size:100%;
  margin-bottom: 17rpx;
}
.hui-upload-img-btn-2 {
  color:#fff;
  width:153rpx;
  height: 50rpx;
  text-align: center;
  line-height: 50rpx;
  font-size:28rpx;
  position: absolute;
  bottom: 30rpx;
  right: 35rpx;
  background: rgba(0,0,0,0.8);
  border-radius: 8rpx;
}

/* 基本信息 */
.hui-form-large-item,.hui-form-small-item{
  text-align: center;
  font-size:36rpx;
  color:#333;
  padding: 34rpx 0;
}
.hui-form-large-item input,.hui-form-small-item input{
  width:100%;
  display: inline-block;
}
.hui-form-large-item .hui-form-item-block{
  position: relative;
  padding: 0 32%;
  margin-top: 14rpx;
}
.hui-form-small-item .hui-form-item-block{
  margin-top: 14rpx;
}
.hui-show-area{
  display: inline-block;
  font-size:20rpx;
  color:#999;
  background: #f2f2f2;
  border-radius: 8rpx;
  height: 30rpx;
  line-height: 30rpx;
  padding: 0 8rpx;
  margin-left: 8rpx;
  position: absolute;
  bottom:15rpx;
  left: 68%;
}
.hui-form-small-item-container{
  border-bottom: 1rpx solid #ececec;
  margin: 0 30rpx;
  border-top: 1rpx solid #ececec;
}
.hui-form-small-item-container .hui-form-small-item{
  width:33.33333333%;
  float:left;
  position: relative;
}
.hui-form-small-item-container.hui-border-top-none{
  border-top:none;
}
.hui-form-small-item-container .hui-form-small-item:after{
  content: '';
  position: absolute;
  width:2rpx;
  background: #ececec;
  height: 88rpx;
  display: block;
  top:50%;
  right: 0rpx;
  margin-top: -44rpx;
}
.hui-form-small-item-container .hui-form-small-item:last-child:after{
  display: none;
}
.hui-tips-1{
  color:#9aa4b3;
  font-size:20rpx;
  padding: 19rpx 0 44rpx;
}
.hui-tips-1:before{
  content: '';
  display: inline-block;
  width:24rpx;
  height: 24rpx;
  background: url(data:image/png;base64,) no-repeat center;
  background-size:100%;
  margin-right: 7rpx;
}
/* 租凭信息 */

.hui-owner-details{
  padding: 34rpx 30rpx;
  margin-bottom: 130rpx;
}
.hui-owner-details .hui-owner-details-txt:before{
  content: '';
  background: url(data:image/png;base64,) no-repeat center;
  display: block;
  width:53rpx;
  height: 59rpx;
  background-size:100% 100%;
  margin-right: 20rpx;
}
.hui-add-owner-details:after{
  content: '';
  display: block;
  width:9rpx;
  height: 17rpx;
  background: url(data:image/png;base64,) no-repeat center;
  background-size:100% 100%;
  margin-left: 12rpx;
}
.hui-next-step{
  position: fixed;
  bottom:0;
  left:0;
  width:100%;
  height:98rpx;
  z-index: 5000;
  background: #fff;
  padding-bottom: env(safe-area-inset-bottom);
}
.hui-next-step .hui-reset{
  box-sizing: border-box !important;
  border-top:1rpx solid #c5c5c5;
  width:110rpx;
  height: 98rpx;
}
.hui-next-step .hui-reset:before{
  content: '';
  display: block;
  width:32rpx;
  height:32rpx;
  background:url(data:image/png;base64,) no-repeat center;
  background-size:100% 100%;
  margin-bottom: 6rpx;
}
.hui-next-step-btn{
  color:#fff;
  text-align: center;
  line-height: 98rpx;
  background: #ffa229;
}
.hui-form-item input{
  width:auto;
}
.hui-sel-icon{
  color:#fe6161;
  font-size:20rpx;
  padding: 0 8rpx;
  height: 30rpx;
  line-height: 30rpx;
  background: #ffe2dc;
  border-radius: 6rpx;
}