.z-compoent-cell{
  height: 120rpx;
  line-height: 120rpx;
  position: relative;
}
.z-component-cell-border:after{
  content: '';
  position: absolute;
  height: 1px;
  bottom: 0;
  left: 0;
  width:100%;
  background: #f2f2f2;
  transform:scaleY(0.5);
}
.z-compoent-cell-item{
  height: 100%;
  font-size: 40rpx;
  color:#000;
  font-weight: 900;
}
.z-compoent-cell-item-right{
  position: absolute;
  right: 0;
  height: 100%;
  top:0;
  display: flex;
  font-size: 24rpx;
  color:#999;
  font-weight: 400;
}
.z-compoent-cell-item-right .z-arrow-box{
  width:30rpx;
  height: 100%;
  display: flex;
  justify-content: center;
	align-items: center;
}
.z-compoent-cell-item-right .z-arrow-box .z-compoent-cell-arrow{
  width:11rpx;
  height: 20rpx;
}