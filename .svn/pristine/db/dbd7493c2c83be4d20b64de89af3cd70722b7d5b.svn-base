page{
    background: #f4f4f4;
    font-size: 30rpx;
}
.list_con{
    background: #fff;
    padding-left: 40rpx;
}
.list_con .list_item{
    padding: 30rpx 30rpx 40rpx 0;
    overflow: hidden;
    height: auto;
    border-bottom: 1rpx solid #e5e5e5;
}
.list_con .list_item:last-child{
    border-bottom: none;
}
.list_con .list_item navigator > view{
    float: left;
}
.list_con .list_item navigator > view.item_left{
    width: 48rpx;
    height: 48rpx;
    box-sizing: content-box;
    padding-right: 24rpx;
    margin-top: 75rpx;
}
.list_con .list_item navigator > view.item_left image{
    width: 48rpx;
    height: 48rpx;
}
.list_con .list_item navigator > view.item_icon{
    float: right;
    width: 12rpx;
    height: 20rpx;
    margin-top: 75rpx;
}
.list_con .list_item navigator > view.item_icon image{
    width: 12rpx;
    height: 20rpx;
}
.list_con .list_item navigator > view.item_right{
    width: 597rpx;
}
.list_con .list_item navigator > view.item_right > text:first-child{
    color: #333;
    font-size:30rpx;
    line-height: 40rpx;
    display: block;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
    font-weight: bold;
}
.list_con .list_item navigator > view.item_right > text:nth-child(3){
    font-size:22rpx;
    line-height: 40rpx;
    color: #656565;
    padding: 10rpx 0 10rpx 0;
    display: block;
}
.list_con .list_item navigator > view.item_right .housedetail{
    font-size:22rpx;
    line-height: 40rpx;
    color: #656565;
    padding: 10px 0 0 0;
    display: block;
}
.list_con .list_item navigator > view.item_right > text text{
    padding-right: 10rpx;
}
.list_con .list_item navigator > view.item_right .paper view{
    line-height: 30rpx;
    text-align: center;
    font-size: 24rpx;
    display: inline-block;
    border-radius: 4rpx;
    color: #31acd5;
    background: #e7faff;
    margin-right: 16rpx;
    padding: 0 10rpx;
}
.list_con .list_item navigator > view.item_right .dianzi view{
    line-height: 30rpx;
    text-align: center;
    font-size: 24rpx;
    display: inline-block;
    border-radius: 4rpx;
    color: #7e97f2;
    background: #f2f0ff;
    margin-right: 16rpx;
    padding: 0 10rpx;
}
.list_con .list_item navigator > view.item_right .dianzi view.red{
    color: #ff0600;
    background: transparent;
}






