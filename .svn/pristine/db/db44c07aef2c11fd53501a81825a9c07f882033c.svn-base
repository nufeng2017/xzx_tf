.z-component-detail-head{
  padding: 40rpx 0 00rpx;
}
.z-component-detail-head-title{
  position: relative;
  padding-right: 220rpx;
}
.z-component-detail-head-title-1{
  padding-right: 0;
}
.z-component-detail-head-title .z-component-detail-head-name{
  line-height: 50rpx;
  font-size: 38rpx;
  font-weight: 900;
  color:#000;
  position: relative;
}
.z-component-detail-head-title .z-component-detail-head-price{
  line-height: 50rpx;
  height: 50rpx;
  position: absolute;
  top:0rpx;
  right: 0rpx;
  text-align: right;
  font-size: 30rpx;
  color:#ff5500;
  font-weight: 900;
}
.z-component-detail-head-address{
  font-size: 24rpx;
  color:#666;
  line-height: 50rpx;
  position: relative;
  padding-right: 60rpx;
}
.z-component-detail-head-address .z-component-detail-head-address-text{
  position: relative;
  padding-left: 40rpx;
}
.z-component-detail-head-address .z-component-detail-head-address-text .z-component-detail-head-address-text-img{
  position: absolute;
  width:40rpx;
  height: 50rpx;
