  top:0;
}
.z-component-detail-head-address .z-component-detail-head-address-text .z-component-detail-head-address-text-img image{
  width:23rpx;
  height: 26rpx;
}
.z-component-detail-head-address-check{
  position: absolute;
  height: 50rpx;
  right: 0;
  top:0;
  width:60rpx;
  text-align: right;
}
.z-component-detail-head-address-check image{
  width:39rpx;
  height: 43rpx;
  display: inline-block;
}
.z-components-item-characteristic{
  padding-top: 10rpx;
  padding-right: 170rpx;
  position: relative;
  min-height: 40rpx;
}
.z-component-detail-head-updata-time{
  position: absolute !important;
  width:170rpx;
  height:20rpx;
  font-size: 20rpx;
  color:#cccccc !important;
  text-align: right;
  bottom: 0 !important;
  right: 0 !important;
  background:#fff !important;
  margin: 0  !important;
  padding: 0 !important;
}