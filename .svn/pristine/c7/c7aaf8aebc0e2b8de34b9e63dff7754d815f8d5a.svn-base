/*清除浮动  */
.cl:after{
  content: '';
  display: block;
  clear:both;
  line-height: 0px;
  height:0px;
  width:0px;
}
.fl{
  float:left;
}
.fr{
  float:right;
}
.inline-block{
  display: inline-block;
}
.text-overflow{
  text-overflow: ellipsis;
  overflow: hidden;
  white-space: nowrap;
  word-break: normal;
}
.hui-mb{
  margin-bottom: 20rpx;
}

/*黄色字体  */
.yellow-font{
  color:#ff9000;
}
/*蓝色字体  */
