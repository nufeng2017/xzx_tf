.hide{
  display: none !important;
}
.ad-f{
	width:17%;
	position: fixed;
	bottom:16%;
	right:20rpx;
}
/*兼容iphonex底部黑条  */
.iphonex{
    bottom:64rpx!important;
}
.iphonex::after{
    content: '';
    width: 100%;
    height: 64rpx;
    background: #fff;
    z-index: 999;
    position: fixed;
    bottom: 0;
    left: 0;
}
/*弹窗  */
.popup-bg{
  position: fixed;
  height:100%;
  width:100%;
  background: rgba(0,0,0,0.4);
  top:0;
  left:0;
  z-index: 100000;
}
.popup-none{
  display: none !important;
}
/*电话 弹窗  */
#check-phone .popup-container{
  background-color:#fff;
  border-radius: 8rpx;
  height:400rpx;
  width:600rpx;
  text-align: center;
  position: relative;
}
#check-phone .popup-title{
  font-size:36rpx;
  padding-top: 50rpx;
  margin-bottom: 22rpx;
}
#check-phone .phone-num{
  font-size:48rpx;
}

#check-phone .phone-num text{
  color:#ff5501;
}
#check-phone .oter-phone-num{
  font-size:26rpx;
  margin-top: 14rpx;
  color:#999;
}
#check-phone .cl{
  position: absolute;
  width:100%;
  height:108rpx;
  border-top:1rpx solid #d1d1d1;
  left:0;
  bottom:0;
  line-height: 108rpx
}
#check-phone .cl:before{
  content: '';
  position: absolute;
  width:2rpx;
  height:108rpx;
  top:0;
  left:50%;
  margin-left: -1rpx;
  background:#d1d1d1;
}
#check-phone .cl>text{
  width:50%;
  color:#666;
  font-size:36rpx;
}
#check-phone .phone-ok{
  color:#35589c !important;
}
.hui-none{
  display: none;
}
.hui-placeholder-style-1{
  font-size:36rpx;
  color:#cccccc;
}
.hui-placeholder-style-2{
  font-size:32rpx;
  color:#cccccc;
}
/* 模态框背景 */
.hui-model-bg{
  position: fixed;
  height: 100%;
  width:100%;
  left: 0;
  top:0;
  background: rgba(0,0,0,0.5);
  z-index: 8000;
  padding-bottom: env(safe-area-inset-bottom);
}
/* 发布租房信息弹窗 */
.hui-popup-container{
  padding-bottom: env(safe-area-inset-bottom);
  position: fixed;
  background: #fff;
  left:0;
  bottom:0;
  width:100%;
  z-index: 10000;
}
.hui-popup-container .hui-popup-header .hui-popup-header-tab .hui-popup-header-tab-item{
  height: 200rpx;
  text-align: center;
  line-height: 2.5;
  position: relative;
  border-right: 1rpx solid #f5f5f5;
}
.hui-popup-container .hui-popup-header .hui-popup-header-tab .hui-popup-header-tab-item:last-child{
  border-right: none;
}
.hui-popup-container .hui-popup-header .hui-popup-header-tab .hui-popup-header-tab-item.hui-sel-1 view{
  color:#ff552e;
}
.hui-popup-container .hui-popup-header .hui-popup-header-tab .hui-popup-header-tab-item.hui-sel-1:after{
  content: '';
  position: absolute;
  height: 6rpx;
  width:100%;
  background: #ff6f29;
  top:0;
  left:0;
}
.hui-popup-container .hui-popup-header .hui-popup-header-tab .hui-popup-header-tab-item.hui-sel-1-1 view{
  color:#ff552e;
}
.hui-popup-container .hui-popup-tips{
  color:#999;
  font-size:28rpx;
  text-align: center;
  position: relative;
  height: 86rpx;
  background: #f9fafc;
  line-height: 86rpx;
}
.hui-popup-container .hui-popup-tips .hui-popup-txt{
  display: inline-block;
  vertical-align: top;
}
.hui-popup-container .hui-popup-tips .hui-popup-btn{
  position: absolute;
  height: 100%;
  width:136rpx;
  right: 0;
  top:0;
  color:#ff6f29;
  font-size:30rpx;
}
.hui-popup-container .hui-popup-tips.hui-popup-tips-style-1{
  padding: 0 160rpx 0 60rpx;
}
.hui-popup-container .hui-popup-tips.hui-popup-tips-style-1 radio-group{
  height: 100%;
  line-height: 86rpx;
  display: inline-block;
  vertical-align: top;
  color:#666;
}
.hui-popup-container .hui-popup-tips.hui-popup-tips-style-1 radio-group radio{
  width:35rpx;
  margin-left: 30rpx;
}
.hui-popup-container .hui-popup-tips.hui-popup-tips-style-1 radio-group radio .wx-radio-input{
  width:30rpx;
  height: 30rpx;
  margin-bottom: 6rpx;
}
.hui-popup-container .hui-popup-header-tab-1{
  text-align: center;
  padding: 0 6rpx;
  height: 100rpx;
}
.hui-popup-container .hui-popup-header-tab-1 .hui-popup-header-tab-1-item{
  margin: 0 6rpx;
  border-radius: 8rpx;
  color:#333;
  font-size:28rpx;
  height: 60rpx;
  line-height: 60rpx;
  background: #f2f2f2;
}
.hui-popup-container .hui-popup-header-tab-1 .hui-popup-header-tab-1-item.hui-sel-this-1{
  color:#fff;
  background: #ff6f29;
}
.hui-popup-container .hui-popup-select-board{
  height: 450rpx;
}
.hui-popup-container .hui-popup-select-board picker-view{
  width:100%;
  height: 350rpx;
}
.hui-popup-container .hui-popup-select-board picker-view view{
  line-height: 50px;
  height: 50px;
  text-align: center;
  font-size:36rpx;
}
.hui-popup-container .hui-keyboard{
  height: 450rpx;
  width:100%;
  padding: 26rpx 0 0;
  background: #f9fafc;
  border-top:1rpx solid #ebebeb;
}
.hui-popup-container .hui-keyboard>view:first-child{
  width:555rpx;
}
.hui-popup-container .hui-keyboard .hui-keyboard-item{
  color:#000;
  font-size:36rpx;
  text-align: center;
  border-radius: 20rpx;
  height: 90rpx;
  line-height: 88rpx;
  border:1rpx solid #efefef;
  width:177rpx;
  box-sizing: border-box;
  margin-left: 8rpx;
  margin-bottom: 8rpx;
  background-color:#fff;
}
.hui-popup-container .hui-keyboard .hui-keyboard-delete,.hui-popup-container .hui-keyboard .hui-keyboard-ok{
  border-radius: 20rpx;
  width:177rpx;
  height: 188rpx;
  text-align: center;
  line-height: 188rpx;
  margin-left: 8rpx;
  margin-bottom: 8rpx;
  background: url(data:image/png;base64,) no-repeat center;
  background-size:36rpx 25rpx;
  background-color:#eaecf0;
}
.hui-popup-container .hui-keyboard .hui-keyboard-ok{
  background:none;
  background-color:#ff6f29;
  color:#fff;
  font-size:32rpx;
}
.hui-popup-container .hui-keyboard .hui-keyboard-cancel{
  background: url(data:image/png;base64,) no-repeat center;
  background-size:32rpx 31rpx;
  background-color:#fff;
}